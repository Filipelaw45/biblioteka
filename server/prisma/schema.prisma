generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id           Int    @id @default(autoincrement())
  username     String @unique
  password_hash String
}

model Books {
  id       Int    @id @default(autoincrement())
  title    String
  author   String
  available Boolean
  isbn     Int    @unique
}